.wrapper
  .search
    = image_tag "larrivee.png", :class => "logo"

    %table{:id => "larrivee", :class => "display", :cellspacing => "0", :width => "100%"}
      %thead
        %tr
          %th{:class => "listing"} Thread
          %th{:class => "replies"} Replies
          %th{:class => "views"} Views
          %th{:class => "last-post"} Last Post
      %tbody
        - cache(cache_key_for_larrivee) do
          = render @larrivees
  .push

